{
  "name": "rag-playground",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "tsc && vite build",
    "build:github": "tsc && vite build --mode github",
    "deploy:github": "pnpm run build:github && pnpm gh-pages -m \"Deploy $(git log '--format=format:%H' main -1)\" -d ./dist",
    "deploy:vercel": "pnpm run build && cp -r .vercel ./dist/ && vercel ./dist",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@floating-ui/dom": "^1.6.1",
    "@mlc-ai/web-llm": "0.2.46",
    "@types/d3-array": "^3.2.1",
    "@types/d3-format": "^3.0.4",
    "@types/d3-random": "^3.0.3",
    "@types/d3-time-format": "^4.0.3",
    "@types/flexsearch": "0.7.3",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@webgpu/types": "^0.1.40",
    "@xiaohk/utils": "^0.0.6",
    "d3-array": "^3.2.4",
    "d3-format": "^3.1.0",
    "d3-random": "^3.0.1",
    "d3-time-format": "^4.1.0",
    "dexie": "^3.2.4",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-lit": "^1.11.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-wc": "^2.0.4",
    "flexsearch": "0.7.3",
    "gh-pages": "^6.1.1",
    "gpt-tokenizer": "^2.1.2",
    "idb-keyval": "^6.2.1",
    "lit": "^3.1.2",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-dts": "^3.7.2",
    "vite-plugin-web-components-hmr": "^0.1.3"
  },
  "dependencies": {
    "@xenova/transformers": "^2.14.2",
    "d3-time-format": "^4.1.0",
    "mememo": "link:../.."
  }
}
